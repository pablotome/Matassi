@{
	ViewBag.Title = "up";
	Layout = "~/Areas/Web/Views/Shared/_LayoutMatassi.cshtml";
}

@section scripts {
	@Styles.Render("~/bundles/estilosCSSModelo")
	@Scripts.Render("~/bundles/funcionesModelo")

	<script type="text/javascript">
		$(document).ready(function () {
			$('#divNovedades').carouselMatassi({ mostrarBotones: !EsCelular(), mostrarIndice: EsCelular() }, function () { /*alert("callback");*/ });
			$('#divCaracteristicas').carouselMatassi({ mostrarBotones: !EsCelular(), mostrarIndice: EsCelular() }, function () { /*alert("callback");*/ });

			$(window).on("orientationchange", function () {
				//window.location.reload();
				setTimeout(function () {
					$('#divNovedades').carouselMatassi({ mostrarBotones: !EsCelular(), mostrarIndice: EsCelular() }, function () { /*alert("callback");*/ });
				}, 500);

				setTimeout(function () {
					$('#divCaracteristicas').carouselMatassi({ mostrarBotones: !EsCelular(), mostrarIndice: EsCelular() }, function () { /*alert("callback");*/ });
				}, 500);
			});
		});
	</script>


	<style type="text/css">
		video {
			width: 100% !important;
			height: auto !important;
		}
	</style>
}

<div id="divBannerTituloModelo">
	<span id="spnTituloModelo">
		Nuevo Virtus
	</span>
</div>
<div id="divBannerTituloCaracteristicasModelo">
	<ul id="ulListaCaracteristicasModelo">
		<li class="liNovedades" data-divmodelos="divNovedades">Novedades</li>
		<li class="liDisenio" data-divmodelos="divDisenio">Diseño</li>
		<li class="liTecnologia" data-divmodelos="divTecnologia">Tecnología</li>
		<li class="liCaracteristicas" data-divModelos="divCaracteristicas">Características</li>
		<li class="liVersiones" data-divmodelos="divVersiones">Versiones</li>
	</ul>
</div>

<div id="divImagenModelo">
	<video controls preload="none" poster="nuevo_virtus.jpg">
		<source src="~/Content/info_modelos/nuevo_virtus/Nuevo_Virtus.mp4" type="video/mp4">
	</video>
</div>

<div id="divTituloModelo">
	Diseñado para ser tuyo
</div>
<div id="divBajadaTituloModelo">

</div>


